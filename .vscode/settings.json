{
  "cSpell.words": [
    "autorun",
    "bendaye",
    "blocknote",
    "bullmq",
    "Checksite",
    "commitlint",
    "corejs",
    "datasource",
    "easings",
    "Flickr",
    "fontsource",
    "hideable",
    "hookform",
    "HTTPJSON",
    "ianvs",
    "inet",
    "Lngs",
    "locize",
    "mantine",
    "maximizable",
    "multistream",
    "NEXTAUTH",
    "nextron",
    "notistack",
    "overlayscrollbars",
    "pino",
    "Signin",
    "superjson",
    "swipeable",
    "systeminformation",
    "tanstack",
    "trpc",
    "ulidx",
    "undocked",
    "unfollow",
    "upsert",
    "usehooks",
    "winget"
  ],
  "editor.formatOnType": true,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.format.enable": true,
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.sortPackageJson": "explicit"
  },
  "editor.stickyScroll.enabled": true,
  "editor.inlineSuggest.enabled": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "search.exclude": {
    ".next": true,
    "dist": true,
    "app": true
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "prisma.fileWatcher": true,
  "i18n-ally.localesPaths": ["apps/web/public/locales", "apps/electron/renderer/public/locales"],
  "i18n-ally.keystyle": "nested",
  "i18n-ally.namespace": true,
  "i18n-ally.sourceLanguage": "en-US",
  "i18n-ally.displayLanguage": "en-US",
  "cSpell.ignorePaths": [
    "package-lock.json",
    "yarn.lock",
    "pnpm-lock.yaml",
    "dist",
    "build",
    ".next",
    ".cache",
    ".vscode",
    ".github",
    ".git",
    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders",
    "*preset.js",
    "tsconfig*.json"
  ],
  "i18n-ally.enabledFrameworks": ["next-intl"]
}
