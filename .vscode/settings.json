{
  "cSpell.words": [
    "autorun",
    "blocknote",
    "bullmq",
    "commitlint",
    "datasource",
    "easings",
    "Flickr",
    "fontsource",
    "hideable",
    "hookform",
    "HTTPJSON",
    "Lngs",
    "locize",
    "mantine",
    "multistream",
    "NEXTAUTH",
    "notistack",
    "overlayscrollbars",
    "pino",
    "Signin",
    "superjson",
    "tanstack",
    "trpc",
    "ulidx",
    "unfollow",
    "upsert",
    "usehooks"
  ],
  "editor.formatOnType": true,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.format.enable": true,
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.sortPackageJson": "explicit",
    "source.organizeImports": "explicit"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "search.exclude": {
    ".next": true,
    "dist": true,
    "app": true
  },
  "editor.stickyScroll.enabled": true,
  "editor.inlineSuggest.enabled": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "prisma.fileWatcher": true,
  "i18n-ally.localesPaths": ["public/locales"],
  "i18n-ally.keystyle": "nested",
  "i18n-ally.namespace": true,
  "i18n-ally.sourceLanguage": "en-US",
  "i18n-ally.displayLanguage": "en-US",
  "i18n-ally.extract.ignored": [
    "Roboto Mono",
    "(max-width: 600px) 100vw, (max-width: 1200px) 50vw, 320px"
  ],
  "i18n-ally.extract.ignoredByFiles": {
    "src/components/dashboard/application/CopyrightSectionCard.tsx": [
      "Copyright"
    ],
    "src/components/dashboard/application/ReadmeSectionCard.tsx": ["Readme"]
  }
}
