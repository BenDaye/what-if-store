{
  "name": "what-if-store",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:* db:studio:dev db:schema:generate",
    "dev:client": "dotenv -e .env.development.local -- next dev",
    "dev:server": "dotenv -e .env.development.local -- tsx watch src/server/dev.ts  --tsconfig tsconfig.server.json ",
    "build": "run-s build:client build:server",
    "build:client": "next build",
    "build:server": "dotenv -e .env.production.local -- tsc --project tsconfig.server.json",
    "start": "dotenv -e .env.production.local -- node dist/prod.js",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" --report-unused-disable-directives --report-unused-disable-directives src",
    "lint-fix": "pnpm lint --fix",
    "prepare": "husky",
    "postinstall": "prisma generate",
    "db:schema:generate": "prisma generate --watch",
    "db:migrate:dev": "dotenv -e .env.development.local -- prisma migrate dev --skip-seed",
    "db:seed:dev": "dotenv -e .env.development.local -- prisma db seed",
    "db:reset:dev": "dotenv -e .env.development.local -- prisma migrate reset --force --skip-seed",
    "db:studio:dev": "dotenv -e .env.development.local -- prisma studio -b none -p 3205",
    "db:migrate:prod": "dotenv -e .env.production.local -- prisma migrate deploy",
    "db:seed:prod": "dotenv -e .env.production.local -- prisma db seed",
    "db:reset:prod": "dotenv -e .env.production.local -- prisma migrate reset --force --skip-seed",
    "db:studio:prod": "dotenv -e .env.production.local -- prisma studio -b none -p 3205",
    "locize:format": "locize format ./public/locales"
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@blocknote/core": "0.12.4",
    "@blocknote/react": "0.12.4",
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.4",
    "@emotion/server": "11.11.0",
    "@emotion/styled": "11.11.5",
    "@faker-js/faker": "8.4.1",
    "@fontsource/roboto": "5.0.12",
    "@fontsource/roboto-mono": "5.0.17",
    "@hookform/error-message": "2.0.1",
    "@hookform/resolvers": "3.3.4",
    "@lexical/react": "0.14.3",
    "@lexical/utils": "0.14.3",
    "@mui/icons-material": "5.15.15",
    "@mui/lab": "5.0.0-alpha.170",
    "@mui/material": "5.15.15",
    "@mui/x-data-grid": "6.19.5",
    "@mui/x-date-pickers": "6.19.5",
    "@next-auth/prisma-adapter": "1.0.7",
    "@prisma/client": "5.12.1",
    "@react-spring/web": "9.7.3",
    "@tanstack/react-query": "5.28.14",
    "@trpc/client": "next",
    "@trpc/next": "next",
    "@trpc/react-query": "next",
    "@trpc/server": "next",
    "@web3-storage/multipart-parser": "^1.0.0",
    "argon2": "0.40.1",
    "bullmq": "5.5.1",
    "countries-list": "3.1.0",
    "currency.js": "2.0.4",
    "date-fns": "3.6.0",
    "i18next": "23.10.1",
    "ioredis": "5.3.2",
    "lexical": "0.14.3",
    "mitt": "3.0.1",
    "next": "14.1.4",
    "next-auth": "4.24.7",
    "next-i18next": "15.2.0",
    "next-multiparty": "0.7.0",
    "notistack": "3.0.1",
    "overlayscrollbars": "2.7.1",
    "overlayscrollbars-react": "0.5.6",
    "pino": "8.19.0",
    "pino-pretty": "11.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.51.2",
    "react-i18next": "14.1.0",
    "react-rnd": "10.4.1",
    "react-swipeable-views": "0.14.0",
    "react-swipeable-views-utils": "0.14.0",
    "serve-handler": "6.1.5",
    "superjson": "2.2.1",
    "swr": "2.2.5",
    "ulidx": "2.3.0",
    "usehooks-ts": "3.1.0",
    "uuid": "9.0.1",
    "ws": "8.16.0",
    "zod": "3.22.4",
    "zod-form-data": "2.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "19.2.1",
    "@commitlint/config-conventional": "19.1.0",
    "@next/eslint-plugin-next": "14.1.4",
    "@tanstack/react-query-devtools": "5.28.14",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^20.12.4",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "@types/react-swipeable-views": "^0.13.5",
    "@types/react-swipeable-views-utils": "^0.13.7",
    "@types/serve-handler": "^6.1.4",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "7.5.0",
    "@typescript-eslint/parser": "7.5.0",
    "dotenv-cli": "7.4.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "9.0.11",
    "locize-cli": "^8.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.2.5",
    "prisma": "5.12.1",
    "tsx": "4.7.2",
    "typescript": "^5.4.4"
  },
  "engines": {
    "node": "^20"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
