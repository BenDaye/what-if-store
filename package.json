{
  "name": "what-if-store",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "dev": "dotenv -e .env.development.local -- turbo run dev",
    "dev:env-check": "dotenv-checker --schema .env.example --env .env.development.local",
    "build": "dotenv -e .env.production.local -- turbo run build",
    "build:env-check": "dotenv-checker --schema .env.example --env .env.production.local",
    "prepare": "husky"
  },
  "resolutions": {
    "@types/node": "20.12.12",
    "@types/react": "18.3.2",
    "@types/react-dom": "18.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "dotenv-checker": "^1.1.5",
    "dotenv-cli": "7.4.2",
    "eslint": "^8.57.0",
    "husky": "9.0.11",
    "npm-run-all": "4.1.5",
    "prettier": "3.2.5",
    "tsx": "4.10.2",
    "turbo": "1.13.3",
    "typescript": "5.4.5"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=20.11.1",
    "pnpm": "8.15.6"
  }
}
