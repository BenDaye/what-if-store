{
  "name": "@what-if-store/prisma",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "main": "index.ts",
  "types": "index.ts",
  "scripts": {
    "build": "run-s build:reset build:migrate build:seed",
    "build:migrate": "dotenv -e .env.production.local -- prisma migrate deploy",
    "build:reset": "dotenv -e .env.production.local -- pnpm run dev:reset",
    "build:seed": "dotenv -e .env.production.local -- pnpm run dev:seed",
    "build:studio": "dotenv -e .env.production.local -- pnpm run dev:studio",
    "dev": "run-s dev:migrate dev:seed dev:studio",
    "dev:migrate": "dotenv -e .env.development.local -- prisma migrate dev --skip-seed",
    "dev:reset": "dotenv -e .env.development.local -- prisma migrate reset --force --skip-seed",
    "dev:seed": "dotenv -e .env.development.local -- prisma db seed",
    "dev:studio": "dotenv -e .env.development.local -- prisma studio -b none",
    "postinstall": "prisma generate && prisma format"
  },
  "dependencies": {
    "@faker-js/faker": "8.4.1",
    "@prisma/client": "5.13.0",
    "argon2": "0.40.1"
  },
  "devDependencies": {
    "@types/node": "20.12.12",
    "prisma": "5.13.0"
  },
  "prisma": {
    "seed": "tsx seed.ts"
  }
}
